<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/authentication_nav"
    app:startDestination="@id/splashScreen2">

    <fragment
        android:id="@+id/splashScreen2"
        android:name="com.example.weather.ui.SplashScreen"
        android:label="fragment_splash_screen"
        tools:layout="@layout/fragment_splash_screen" >
        <action
            android:id="@+id/action_splashScreen2_to_login2"
            app:destination="@id/login2" />
        <action
            android:id="@+id/action_splashScreen2_to_teamsCondition2"
            app:destination="@id/teamsCondition2" />
    </fragment>
    <fragment
        android:id="@+id/login2"
        android:name="com.example.weather.ui.Login"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" />
    <fragment
        android:id="@+id/teamsCondition2"
        android:name="com.example.weather.ui.TeamsCondition"
        android:label="fragment_teams_condition"
        tools:layout="@layout/fragment_teams_condition" />
</navigation>